# root
mount -ac #s/ramboot /root $rootspec
bind -a $rootdir /
bind -c $rootdir/mnt /mnt

# kernel devices
bind #c /dev
bind #d /fd
bind -c #e /env
bind #p /proc
bind -c #s /srv
bind -a #Â¤ /dev
bind -a #S /dev

# mount points
mount -a /srv/slashn /n

# authentication
mount -a /srv/factotum /mnt

# standard bin
bind -b /boot /bin
bind -a /root/bin /bin
bind -a /root/bin /boot
bind -a /$cputype/bin /bin
bind -a /rc/bin /bin

# internal networks
# mount -a /srv/ip /net
bind -a #l /net
bind -a #I /net
mount -a /srv/cs /net
mount -a /srv/dns /net
mount -a /srv/net /net

mount -c /srv/hubfs /n/hubfs

bind -c /usr/$user/tmp /tmp
cd /usr/$user

. /lib/namespace.local
. /lib/namespace.$sysname
. /cfg/$sysname/namespace
